{
    "manifest_version": 3,
    "name": "Tokito",
    "version": "1.0",
    "description": "Crypto non-custodial wallet",
    "action": {
      "default_popup": "index.html",
      "default_icon": "/icon.jpg"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "host_permissions": ["*://*.ethereum.org/*"],
    "background": {
      "service_worker": "background.js"
    }
  }
  